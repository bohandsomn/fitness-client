{
    "usesTypeScript": true,
    "usesCssModule": false,
    "cssPreprocessor": "css",
    "testLibrary": "None",
    "usesStyledComponents": false,
    "component": {
        "default": {
            "path": "src/shared/ui",
            "withComponent": true,
            "withPublicApi": true,
            "withStory": true,
            "withHook": true,
            "withType": true,
            "withStyle": false,
            "withTest": false,
            "withLazy": false,
            "customTemplates": {
                "publicApi": "src/shared/generate/templates/ui/index.ts",
                "component": "src/shared/generate/templates/ui/TemplateName.tsx",
                "story": "src/shared/generate/templates/ui/TemplateName.stories.tsx",
                "hook": "src/shared/generate/templates/ui/useTemplateName.ts",
                "type": "src/shared/generate/templates/ui/type.ts"
            }
        },
        "api": {
            "path": "src/shared/api",
            "withComponent": false,
            "withPublicApi": true,
            "withType": true,
            "withDto": true,
            "withApi": true,
            "customTemplates": {
                "dto": "src/shared/generate/templates/api/dto/index.ts",
                "publicApi": "src/shared/generate/templates/api/index.ts",
                "api": "src/shared/generate/templates/api/templateName.ts",
                "type": "src/shared/generate/templates/api/type.ts"
            }
        }
    }
}
